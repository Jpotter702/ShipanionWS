# Authentication Configuration
# Copy this file to .env and customize as needed

# JWT Secret Key (used to sign tokens)
# Generate a secure key with: openssl rand -hex 32
JWT_SECRET_KEY=your_secure_secret_key_here

# JWT Token Expiration (in minutes)
# ACCESS_TOKEN_EXPIRE_MINUTES=30

# Test Token (pre-generated token for testing)
# Override the default test token if needed
# WS_TEST_TOKEN=your_custom_test_token

# ShipVox API Configuration
# URL of the ShipVox API
SHIPVOX_API_URL=http://localhost:8000/api
